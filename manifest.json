{
  "manifest_version": 2,
  "name": "SDO Tests Helper",
  "description": "Скатай ответы используя очко",
  "version": "0.0.1",
  "author": "SpringFlash",
  "browser_action": {
    "default_title": "Ответы к тесту, автоматик",
    "default_icon": "question.png",
    "default_popup": "output.html"
  },
  "icons": {
    "512": "question.png"
  },
  "content_scripts": [
    {
      "matches": [
        "file:///*",
        "https://sdo.srspu.ru/mod/quiz/review*"
      ],
      "js": [
        "scores.js"
      ]
    },
    {
      "matches": [
        "file:///*",
        "https://sdo.srspu.ru/mod/quiz/attempt*"
      ],
      "js": [
        "answer.js"
      ]
    }
  ],
  "permissions": [
    "clipboardWrite"
  ]
}